{
  "name": "video-content-generator",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@content-generators/ui-components": "^1.0.1-PRE",
    "gh-pages": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "prepare": "npm update @content-generators/ui-components",
    "prestart": "cp -R ./node_modules/@content-generators/ui-components/dist/public/ ./public/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild":"cp -R ./node_modules/@content-generators/ui-components/dist/public/ ./build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "tailwindcss": "^3.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
